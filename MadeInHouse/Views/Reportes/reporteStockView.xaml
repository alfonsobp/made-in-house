<UserControl xmlns:Layouts="clr-namespace:MadeInHouse.Views.Layouts"
x:Class="MadeInHouse.Views.Reportes.reporteStockView"
xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
