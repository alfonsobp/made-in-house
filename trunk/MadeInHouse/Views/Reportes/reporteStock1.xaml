<Window x:Class="MadeInHouse.Views.Reportes.reporteStock1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:rv="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"
        Title="reporteStock1" Height="500" Width="800">
    <Grid Margin="0,0,2,7">
        <WindowsFormsHost Margin="10,68,10,10" Grid.RowSpan="2">
            <rv:ReportViewer x:Name="reportViewer"/>
        </WindowsFormsHost>
        <Label Content="Tienda" HorizontalAlignment="Left" Margin="28,10,0,0" VerticalAlignment="Top" Height="26" Width="45"/>
        <Label Content="Almacen" HorizontalAlignment="Left" Margin="188,10,0,0" VerticalAlignment="Top" Height="26" Width="56"/>
        <Button  Content="OK" HorizontalAlignment="Left" Margin="329,18,0,0" VerticalAlignment="Top" Width="53" Height="45" Click="Button_Click_1"/>
        <ComboBox Name="cmbTienda" ItemsSource="{Binding}" DisplayMemberPath="Nombre" SelectedValuePath="IdTienda" HorizontalAlignment="Left" Margin="28,41,0,0" VerticalAlignment="Top" Width="120" Height="22"/>
        <ComboBox Name="cmbAlmacen" HorizontalAlignment="Left" Margin="188,41,0,0" VerticalAlignment="Top" Width="120" Height="22"/>
    </Grid>
</Window>
